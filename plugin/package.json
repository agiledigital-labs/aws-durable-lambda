{
  "name": "@agiledigital/aws-durable-lambda",
  "version": "1.0.0",
  "description": "A Serverless plugin to automate using AWS SQS, AWS DynamoDB to achieve a durable function",
  "main": "lib/aws-durable-lambda.js",
  "types": "lib/libs/types.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "prepare": "run-s build",
    "build": "rimraf ./lib && tsc && node ./esbuild.config.js",
    "format": "prettier --write src",
    "format-check": "prettier --check src"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "fs-extra": "^10.1.0",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "aws-sdk": ">=2.1042.0",
    "dynamoose": ">=2.8.3"
  },
  "devDependencies": {
    "@serverless/typescript": "3.21.0",
    "@types/aws-lambda": "8.10.102",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "16.11.47",
    "@types/uuid": "8.3.4",
    "aws-sdk": "2.1191.0",
    "dynamoose": "2.8.6",
    "esbuild": "0.15.0",
    "esbuild-node-externals": "^1.4.1",
    "esbuild-node-tsc": "1.8.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "typescript": "4.7.4"
  },
  "author": "Agile Digital",
  "license": "MIT"
}
